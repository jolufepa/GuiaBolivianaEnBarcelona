{
  "navigationFallback": {
    "rewrite": "/index.html",
    "exclude": [ "/images/*", "/css/*" ]
  },
  "mimeTypes": {
    ".json": "application/json",
    ".wasm": "application/wasm",
    ".dll": "application/octet-stream",
    ".js": "application/javascript",
    ".dat": "application/octet-stream",
    ".blat": "application/octet-stream"
  },
  "responseOverrides": {
    "404": {
      "rewrite": "/index.html"
    }
  },
  "globalHeaders": {
    "content-security-policy": "default-src 'self' 'unsafe-eval' 'unsafe-inline' https: data:;"
  }
}